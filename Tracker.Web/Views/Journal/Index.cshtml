@model Dictionary<Tracker.Core.Learner, int>
<h2>Class Journal</h2>
<a asp-action="Create" class="btn btn-success mb-3">Add New Student</a>
<table class="table">
    <thead><tr><th>Name</th><th>Email</th><th>Grade</th></tr></thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Key.FullName</td>
                <td>@item.Key.Email</td>
                <td>@item.Value</td>
            </tr>
        }
    </tbody>
</table>